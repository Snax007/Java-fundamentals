class Student 
{
    String name;
    int age;
    
    // Constructor - parameter names same as instance variables
    Student(String name, int age) {
        this.name = name;  // 'this.name' refers to instance variable
        this.age = age;    // 'age' on right is parameter
    }
}


class Rectangle 
{
    int length;
    int width;
    
    // Default constructor
    Rectangle() 
    {
        this(10, 20);  // Calls the parameterized constructor
    }
    
    // Parameterized constructor
    Rectangle(int length, int width) 
    {
        this.length = length;
        this.width = width;
    }
}
